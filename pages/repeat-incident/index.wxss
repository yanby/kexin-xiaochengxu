.tab-model{
    width: 100%;
    height: 100%;
}

.tabs-wrap{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.tabs-inner{
  display: flex;
  width: 100%;
}


.tabs-item{
  flex: 1;
  display: inline-block;
  margin: 0 10rpx;
  height: 88rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 88rpx;
  color: #BFBFBF;
  border-bottom: 2rpx solid #fff;
}
.tabs-item.cur{
  color: #5BC177;
  border-bottom: 2rpx solid #5BC177;
}

.page-farm-work-repeat {
  width: 750rpx;
}
.close-tab{
  position: absolute;
  right: 0;
  top: 0;
  width: 80rpx;
  height: 80rpx;
  font-size: 40rpx;
  text-align: center;
  line-height: 80rpx;
}

.page-content {
  width: 684rpx;
  margin: 0 auto;

}

.page-title {
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:600;
  line-height:42rpx;
  color:rgba(89,87,87,1);
  margin-bottom: 28rpx;
  padding-top: 30rpx;
}
.one{
  display: flex;
  /* justify-content: space-between; */
  flex-wrap:wrap;
  padding: 0 30rpx 0rpx;
}
.one-item{
  width: 320rpx;
  height: 55rpx;
  /* padding: 10rpx 30rpx; */
  text-align: center;
  line-height: 55rpx;
  margin: 15rpx 8rpx;
  border-radius: 10rpx;
  border: 1rpx solid #BFBFBF;
  color: #BFBFBF;
  font-size: 28rpx;

}
.one-item.cur{
  color: #5BC177;
  border: 1rpx solid #5BC177;
}
.page-card {
  width:612rpx;
  box-shadow:0 0 12rpx rgba(196,196,196,0.16);
  border-radius:12rpx;
  padding: 34rpx 36rpx 40rpx;
  margin: 0 auto;
}
.page-history{
   width: 684rpx;
  margin: 70rpx auto;
}
.history-list{
  padding: 20rpx 0;
  width:550rpx;
  margin: 0 auto;
}
.list{
  width:550rpx;
  height:55rpx;
  line-height: 55rpx;
  text-align: center;
  color: #BFBFBF;
  font-size: 30rpx;
  border:1px solid #BFBFBF;
  border-radius:12rpx;
}
.list text{
  color: #5BC177;
}
.history-title{
  font-size: 30rpx;
  color: #908D8D;
  margin-bottom: 20rpx;
}
.work-item {
  display: inline-block;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:600;
  padding: 6rpx 0;
  color:rgba(191,191,191,1);
  width:186rpx;
  border:1rpx solid rgba(191,191,191,1);
  border-radius:12rpx;
  text-align: center;
  margin-right: 20rpx;
}

.work-item-active0 {
  color:rgba(243,151,0,1);
  border:1rpx solid rgba(243,151,0,1);
}

.work-item-active1 {
  color:rgbargba(91,193,119,1);
  border:1rpx solid rgba(91,193,119,1);
}

.page-bot {
  width: 750rpx;
}

.page-logo {
  width: 750rpx;
  height: 45rpx;
  margin-top: 34rpx;
  margin-bottom: 14rpx;
  text-align: center;
}

.page-logo image {
  width: 190rpx;
  height: 45rpx;
  background-size: 100%;
}

.page-button {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight :400;
  color: rgba(191,191,191,1);
  width: 750rpx;
  text-align: center;
}

.page-button .button {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 600;
  line-height: 56rpx;
  color: rgba(255,255,255,1);
  width: 686rpx;
  padding: 20rpx 0 18rpx;
  margin: 0 auto;
  text-align: center;
  background: #5BC177;
  border-radius: 6rpx;
  margin-top: 92rpx;
}