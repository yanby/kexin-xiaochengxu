.complete{
  background: #fff;
}
.complete-wrap{
  width: 600rpx;
  margin: 0 auto;
}
.complete-title{
  padding-top: 64rpx;
  padding-bottom: 64rpx;
}
.complete-img {
  width: 64rpx;
  margin: 0 auto 46rpx;
}
.complete-img image{
  width: 64rpx;
  height: 64rpx;
}
.time{
  font-size: 32rpx;
  text-align: center;
  color: #BFBFBF;
}
.time-cur{
  padding-top: 20rpx;
  font-size: 32rpx;
  text-align: center;
  color: #BFBFBF;
}
.leixing{
  font-size: 30rpx;
  font-weight: bold;
  height: 146rpx;
  line-height: 146rpx;
  border-top: 1rpx solid #EAEAEA;
}
.tuwen{
  padding: 46rpx 0; 
  border-top: 1rpx solid #EAEAEA;
}
.tuwen-img{
  display: flex;
  flex-wrap: wrap;
}
.tuwen-img image{
  width:182rpx;
  height:115rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.tuwen-txt view:nth-of-type(1){
  font-size: 28rpx;
  padding-top: 15rpx;
  margin-bottom: 5rpx;
}
.tuwen-txt view:nth-of-type(2){
  font-size: 28rpx;
  color: #BFBFBF;
}
.touruping{
  padding-bottom: 44rpx;
  border-top: 1rpx solid #EAEAEA;
}
.touruping-txt{
  padding: 28rpx;
  font-size: 28rpx;
}
.touruping-list{
  display: flex;
  justify-content: space-between;
}
.touruping-img{
  width:278rpx;
  height:175rpx;
}
.touruping-img image{
  width: 100%;
  height: 100%;
}
.leibie{
  height: 114rpx;
  line-height: 114rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #EAEAEA;
}
.nocomplate-button {
  display: flex;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight :400;
  color: rgba(191,191,191,1);
  text-align: center;
  padding-bottom: 40rpx;
}

.complate-yes,.complate-no {
  font-size: 40rpx;
  font-family: PingFang SC;
  line-height: 56rpx;
  color: rgba(255,255,255,1);
  flex: 1;
  padding: 20rpx 0 18rpx;
  text-align: center;
  background: #5BC177;
  border-radius: 6rpx;
  margin-top: 92rpx;
}
.complate-yes{
  margin-right: 30rpx;
}
.weiwancheng{
  text-align: center;
  font-size: 30rpx;
  color: #f00;
}