.find-farm{
  width: 750rpx;
  margin: 0 auto;
  background: #fff;
  height: 100vh;
}
.wrap{
  padding: 150rpx 0;
}
.title{
  font-size: 40rpx;
  text-align: center;
  font-weight: bold;
}
.scroll{
  width: 600rpx;
  height: 200rpx;
  margin: 100rpx auto;
  white-space: nowrap;
  display: flex
}

.tabs-item{
  width: 198rpx;
  height: 198rpx;
  background: #000;
  border: 1rpx solid transparent;
  display: inline-block;
  margin: 0 20rpx;
}
.tabs-item.cur{
  border: 1rpx solid #5BC177;
}

.goFarm{
  width:690rpx;
  height:88rpx;
  margin: 0 auto;
  line-height: 88rpx;
  text-align: center;
  background:#5BC177;
  border-radius:10rpx;
  color:#fff;
}
.creatfarm{
  font-size: 28rpx;
  text-align: center;   
  padding-top: 90rpx;
}